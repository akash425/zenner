[mongodb]
# MongoDB Atlas connection string
# Format: mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority
# You can also set MONGO_URI environment variable as a fallback
uri = mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority

[database]
# Database name
name = lorawan

# Collection name
collection = uplinks

[ingestion]
# CSV file path
csv_file_path = ./data/lorawan_uplink_devices.csv

# Log file path
log_file_path = ./logs/ingestion.log

# Batch size for MongoDB bulk operations
batch_size = 1000

